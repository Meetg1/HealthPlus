<div class="modal-body">
    <!-- <div class="text-right"> <i class="fa fa-close close" data-dismiss="modal"></i> </div> -->
    <div class="row">
        <div class="col-md-6">
            <div class="text-center mt-1"> <img src="https://i.imgur.com/zZUiqsU.png" width="200"> </div>
        </div>
        <div class="col-md-6">
            <div class="text-white mt-4">
                <!-- <span class="intro-1">This is fake Data</span> -->
                <div class="mt-2"> <span class="intro-2" style="font: bolder;">Thankyou for consultation
                        with HealthPlus.</span> </div>
                <form method="POST" action="/feedback/<%= appointmentid%>">
                    <div class="intro-3">Please give us your valuable review. </div>
                    <div class="rating">
                        <input type="radio" id="star1" name="rating" value="1" /><label for="star1"></label>
                        <input type="radio" id="star2" name="rating" value="2" /><label for="star2"></label>
                        <input type="radio" id="star3" name="rating" value="3" /><label for="star3"></label>
                        <input type="radio" id="star4" name="rating" value="4" /><label for="star4"></label>
                        <input type="radio" id="star5" name="rating" value="5" /><label for="star5"></label>
                    </div>
            </div>
        </div>
    </div>
</div>


<div class="text-center">
    <div class="wrapper">
        <input type="radio" name="select" id="option-1" value="true">
        <input type="radio" name="select" id="option-2" value="false">
        <label for="option-1" class="option option-1">
            <!-- <div class="tick"></div> -->
            <span>I Recommend Doctor</span>
        </label>
        <label for="option-2" class="option option-2">
            <!-- <div class="cross"></div> -->
            <span>I don't Recommend Doctor</span>
        </label>
    </div>
</div>

<br>

<div class="react-tabs__tab-panel react-tabs__tab-panel--selected" role="tabpanel" id="react-tabs-75459"
