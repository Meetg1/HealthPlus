    <div class="c-profile--sections">
        <div class="d-flex flex-row add-comment-section mt-4 mb-4">
            <!-- <img class="img-fluid img-responsive rounded-circle mr-2" src="https://i.imgur.com/qdiP4DB.jpg" width="38"> -->
            <input type="text" class="form-control mr-3" placeholder="Reason to Consult.." name="ConsultReason"><br>
            <input type="text" class="form-control mr-3" placeholder="Your Feedback" name="Feedback">
            <!-- <button
                            class="comment" type="button">Comment</button> -->
            <button href="/" class="comment" type="submit">Comment</button>
        </div>
        </form>
        <div class="feedback--list-container">
            <% for (var i=0; i < review.length; i++) { %>
                <div>
                    <div class="pure-g feedback--item u-cushion--medium-vertical" data-qa-id="feedback_item">
                        <div class="pure-u-2-24">
                            <div class="feedback__icon" data-qa-id="reviewer-image"
                                style="background-color: rgb(255, 249, 179);">A</div>
                        </div>
                        <div class="pure-u-22-24 u-lheight-default">
                            <div
                                class="u-d-flex flex-wrap flex-jc-space-between u-gap-12 u-cushion--small-top u-cushion--bottom">
                                <div class="u-d-flex flex-wrap u-gap-12">
                                    <div class="u-color--grey3"><span class="u-bold" data-qa-id="reviewer-name">
                                            <%= review[i].patientname %>
                                        </span>&nbsp;<span class="u-bold"></span></div>
                                    <div><span
                                            class="u-blue-2-background u-smallest-font u-color--white u-round-corner--12 u-cushion--right-less u-cushion--small-left u-cushion--small-top u-cushion--small-bottom"></span>
                                    </div>
                                </div>
                                <span data-qa-id="web-review-time"
                                    class="u-spacer--left-thin u-f-right u-color--grey3"><i class="fa fa-star"></i><i
                                        class="fa fa-star"></i><i class="fa fa-star"></i><i
                                        class="fa fa-star"></i></span>

                            </div>
                            <div class="feedback__body">
                                <p class="feedback__content u-cushion--bottom u-title-font u-bold"
                                    data-qa-id="visited-for"><span><span>Visited
                                            For</span>&nbsp;</span><span class="procedure">
                                        <%= review[i].consult %>
                                    </span></p>
                                <% if(review[i].recommend==true) { %>
                                    <div class="u-cushion--small-bottom u-large-font"><span><img
                                                src="https://cdn-icons-png.flaticon.com/512/1828/1828640.png"
                                                width="17px" height="17px"></span><span
                                            style="font-weight: 500;margin-left: 6px;">Doctor
                                            Recommended</span></div>
                                    <% } else { %>
                                        <div class="u-cushion--small-bottom u-large-font"><span><img
                                                    src="https://cdn-icons-png.flaticon.com/512/1828/1828640.png"
                                                    width="17px" height="17px"></span><span
                                                style="font-weight: 500;margin-left: 6px;">Doctor
                                                Not Recommended</span></div>
                                        <% } %>
                                            <!-- <p class="feedback__content u-cushion--small-bottom u-large-font" data-qa-id="happy-with"><span><span>Happy with:</span>&nbsp;</span><span class="feedback__context">Doctor friendliness</span><span class="feedback__context">Explanation of the health issue</span><span class="feedback__context">Treatment satisfaction</span></p> -->
                                            <div>
                                                <p class="feedback__content u-large-font " data-qa-id="review-text">
                                                    <%= review[i].feedback %>
                                                </p>
                                            </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% } %>
                    <button data-qa-id="view-more-feedback" class="c-btn--dark feedback__pagination-btn"><span>Read
                            More</span></button>
        </div>
    </div>
</div>
